<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.*?>
<StackPane fx:id="stackPane" focusTraversable="true" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.40"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="GameController">
    <children>
        <ProgressIndicator/>
        <GridPane fx:id="gridPane" focusTraversable="true" maxWidth="1.7976931348623157E308" opacity="0.2">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="150.0" prefWidth="150.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="515.0" minWidth="10.0" prefWidth="515.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <TableView fx:id="infoTable" focusTraversable="false" prefHeight="200.0" prefWidth="100.0"
                           GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                    <columns>
                        <TableColumn fx:id="playerColumn" prefWidth="75.0" text="Player"/>
                        <TableColumn fx:id="numColumn" prefWidth="75.0" text="#Treasure"/>
                    </columns>
                </TableView>
            </children>
        </GridPane>
    </children>
</StackPane>
